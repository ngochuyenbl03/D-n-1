{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent",
          "@-ms-viewport"
        ]
      }
    ],
    "axe/language": "off",
    "meta-viewport": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "meta-charset-utf-8": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 107",
    "not and_ff <= 106",
    "not chrome <= 107",
    "not edge <= 107",
    "not firefox <= 106",
    "not ios_saf <= 16.1",
    "not opera <= 91",
    "not safari <= 16.1",
    "not samsung <= 18"
  ]
}